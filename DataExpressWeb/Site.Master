                $(document).ready(function () {
                    $("div.dropzone").each(function () {
                        var _url = $(this).attr('url');
                        var _urlAjax = $(this).attr('urlAjax');
                        var _maxFiles = $(this).attr('maxFiles');
                        var _maxFileSize = $(this).attr('maxFileSizeMb');
                        var _message = $(this).attr('message');
                        var _extraParam = $(this).attr('extraParam');
                        var _blockedButton = $(this).attr('idBlockedButton');
                        $(this).dropzone({
                            paramName: "file",
                            params: {
                                parameter: _extraParam || null
                            },
                            url: _url,
                            maxFilesize: _maxFileSize || 10, // MB
                            maxFiles: _maxFiles || 10,
                            fallback: null,
                            dictDefaultMessage: _message || "Drop files here or click to upload.",
                            autoProcessQueue: true,
                            init: function () {
                                var myDropZone = this;
                                myDropZone.on("maxfilesexceeded", function (data) {
                                    alertBootBox("Error al subir archivo '" + file.name + "': Se ha excedido el número máximo de archivos", 4);
                                });
                                myDropZone.on("processing", function (file) {
                                    //alert("on processing");
                                });
                                myDropZone.on("complete", function (file) {
                                    var _this = this;
                                    if (_urlAjax != undefined && _urlAjax != "") {
                                        if (_blockedButton != undefined && _blockedButton != "") {
                                            var element = document.getElementById(_blockedButton);
                                            element.style.display = "none";
                                        }
                                        var accepted = myDropZone.getAcceptedFiles();
                                        var outterArray = [];
                                        var inserted = false;
                                        for (var i = 0, len = accepted.length; i < len; i++) {
                                            var fileLoop = accepted[i];
                                            getBase64(fileLoop, function (fileBase64, fileName) {
                                                var innerArray = [fileName, fileBase64];
                                                outterArray.push(innerArray);
                                                inserted = outterArray.length == accepted.length;
                                            });
                                        }
                                        setTimeout(function () {
                                            if (inserted == true) {
                                                var result = sendFilesFromDropZone(_extraParam, outterArray);
                                                if (!result) {
                                                    _this.removeFile(file);
                                                }
                                                if (_blockedButton != undefined && _blockedButton != "") {
                                                    var element = document.getElementById(_blockedButton);
                                                    element.style.display = "block";
                                                }
                                            }
                                        }, 500);
                                    }
                                });
                                myDropZone.on("success", function (file, serverResponse) {
                                    try {
                                        // var removeButton = file.previewElement.querySelector("button#removeButton");
                                        // file.previewElement.removeChild(removeButton);
                                    } catch (err) {
                                        // alert("Error al eliminar button#removeButton: " + err);
                                    } finally {
                                        // alertBootBox(serverResponse, 1);
                                    }
                                });
                                myDropZone.on("removedfile", function (file) {
                                    // alert("Archivo '" + file.name + "' eliminado");
                                });
                                myDropZone.on("error", function (file, errorMessage) {
                                    alertBootBox("Error al subir archivo '" + file.name + "': " + errorMessage, 4);
                                    this.removeFile(file);
                                });
                                myDropZone.on("addedfile", function (file) {
                                    var removeButton = Dropzone.createElement("<button id='removeButton' class='btn btn-danger btn-xs'><i class='fa fa-times' aria-hidden='true'></i>&nbsp;Quitar</button>");
                                    var _this = this;
                                    removeButton.addEventListener("click", function (e) {
                                        e.preventDefault();
                                        e.stopPropagation();
                                        _this.removeFile(file);
                                    });
                                    file.previewElement.appendChild(removeButton);
                                });
                            }
                        });
                    });
                });
