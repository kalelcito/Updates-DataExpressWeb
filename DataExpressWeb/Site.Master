        <script type="text/javascript">
            Sys.WebForms.PageRequestManager.getInstance().add_endRequest(function () { });
            Sys.Application.add_load(bindEvents);
            Sys.Application.add_init(appl_init);
            function appl_init() {
                var pgRegMgr = Sys.WebForms.PageRequestManager.getInstance();
                pgRegMgr.add_beginRequest(beforeAsyncPostBack);
                pgRegMgr.add_endRequest(afterAsyncPostBack);
                Sys.Net.WebRequestManager.add_invokingRequest(beforePostBack);
                Sys.Net.WebRequestManager.add_completedRequest(afterPostBack);
            }
            function beforeAsyncPostBack(sender, e) {
                localStorage.setItem('activeAccordionGroup', '');
                var active = [];
                $("[id*='accordion'] .in").each(function () {
                    active.push($(this).attr('id'));
                });
                localStorage.setItem('activeAccordionGroup', JSON.stringify(active));
            }
            function afterAsyncPostBack(sender, e) {
                var data = JSON.parse(localStorage.getItem("activeAccordionGroup"));
                if (data != undefined && data != null && data != []) {
                    $("[id*='accordion'] .panel-collapse").removeClass('in');
                    $.each(data, function (index, value) {
                        $("#" + value).addClass("in");
                    });
                }
            }
            function beforePostBack(sender, e) {
                beforeAsyncPostBack(sender, e);
                $("div#divLoading").addClass('show');
            }
            function afterPostBack(sender, e) {
                afterAsyncPostBack(sender, e);
                $("div#divLoading").removeClass('show');
            }
            function bindEvents() {
                $.fn.hasAttr = function (name) {
                    return this.attr(name) !== undefined;
                };
                $.fn.redraw = function () {
                    $(this).each(function () {
                        var redraw = this.offsetHeight;
                    });
                };
                $(document).ready(function () {
                    var defaultColor = '<%= System.Configuration.ConfigurationManager.AppSettings["ColorDefault"] != null ? System.Configuration.ConfigurationManager.AppSettings["ColorDefault"].ToString() : "90,175,212" %>';
                    if (defaultColor == undefined || defaultColor == null || defaultColor == '') {
                        defaultColor = '90,175,212';
                    }
                    $('#bodyMaster').css('background', 'linear-gradient(rgba(' + defaultColor + ',0.75),rgba(' + defaultColor + ',0.75)),url("' + ResolveUrl('~/imagenes/backgrounds/trans.jpg') + '") no-repeat center center fixed !important');
                });
                $(document).ready(function () {
                    $('#rootwizard').bootstrapWizard({
                        tabClass: 'nav nav-pills nav-noaction',
                        onNext: function (tab, navigation, index) {
                            localStorage.setItem('lastTab', index);
                            return (Boolean(validateTab(index)));
                        },
                        onTabShow: function (tab, navigation, index) {
                            var $total = navigation.find('li').length;
                            var $current = index + 1;
                            var $percent = ($current / $total) * 95;
                            $('#rootwizard .progress-bar').css({ width: $percent + '%' });
                        },
                        onTabClick: function (tab, navigation, index) {
                            return false; // <---- COMENTAR PARA TESTING
                        },
                        onPrevious: function (tab, navigation, index) {
                            localStorage.setItem('lastTab', index);
                        },
                        onFirst: function (tab, navigation, index) {
                            localStorage.setItem('lastTab', index);
                        },
                        onLast: function (tab, navigation, index) {
                            localStorage.setItem('lastTab', index);
                            return (Boolean(validateTab(index)));
                        }
                    });
                });
                $(document).ready(function () {
                    $("input[type='checkbox']").each(function () {
                        var chk = $(this);
                        var checkStrings = 'SI|NO';
                        var span = chk.closest("span");
                        if (span.hasAttr("checkStrings")) {
                            checkStrings = span.attr("checkStrings");
                        }
                        var size = span.attr("size") || 'mini';
                        var splitted = checkStrings.split('|', 2);
                        var onString = splitted[0];
                        var offString = splitted[1];
                        chk.bootstrapToggle({
                            on: onString,
                            off: offString,
                            size: size
                        });
                        chk.change(function () {
                            chk.click();
                        });
                        var $label = $("label[for='" + chk.attr("id") + "']");
                        var labelText = $label.text();
                        if (!startsWith(labelText, '\xa0')) {
                            $label.text('\xa0' + labelText + '\xa0');
                        }

                    });
                });
                $(document).ready(function () {
                    var chkPostBack = '<%= Page.IsPostBack ? "true" : "false" %>';
                    if (chkPostBack == 'true') {
                        var lastTab = localStorage.getItem('lastTab');
                        if (lastTab != undefined) {
                            $('#rootwizard').bootstrapWizard('show', lastTab);
                        }
                    } else {
                        clearNotifications();
                    }
                });
                $(document).ready(function () {
