            function ReformatTextEditor() {
                var txtAntes = $('#summernoteDiv').summernote('code');
                if (endsWith(txtAntes, "</p>")) {
                    txtAntes = txtAntes.replace(/<p>(?![\s\S]*<p>)/, '').replace(/<\/p>(?![\s\S]*<\/p>)/, '');
                }
                if (endsWith(txtAntes, "<br>")) {
                    txtAntes = txtAntes.replace(/<br>(?![\s\S]*<br>)/, '');
                }
                return txtAntes;
            }
