            function validateFinish(buttonId) {
                var index = ($('#rootwizard').bootstrapWizard('currentIndex')) + 1; //ZERO based index
                var valid = (Boolean(validateTab(index)));
                if (valid) {
                    localStorage.setItem('lastTab', 0);
                    $('#rootwizard .progress-bar').css({ width: '100%' });
                    SimulateClick(buttonId);
                    $('#rootwizard').bootstrapWizard('show', 0);
                }
                return valid;
            }
            function LimpiarFiltros(idBody, idModal) {
                $('#' + idBody + ' input[type="text"]').val('');
                $('#' + idBody + ' select').each(function () {
                    var defaultVal = $(this).prop('defaultSelected');
                    $(this).val(defaultVal);
                });
                $('#' + idModal).modal('show');
                return false;
            }
            function SimulateClick(buttonId) {
                var button = document.getElementById(buttonId);
                if (button) {
                    if (button.click) {
                        button.click();
                    } else if (button.onclick) {
                        button.onclick();
                    }
                }
            }
            function validateTab(index) {
                var mensajes = [];
                var control = true;
                $("#tab" + index + " .validationGroup").each(function () {
                    if ($(this).hasAttr("habilitado")) {
                        var valid = Page_ClientValidate($(this).attr("value"));
                        if (!valid) {
                            control &= false;
                            if ($(this).hasAttr("mensaje")) {
                                mensajes.push($(this).attr("mensaje"));
                            }
                        }
                    }
                });
                if (!control) {
                    if (mensajes.length == 0) {
                        mensajes.push("Verifique que todos los datos sean correctos");
                    }
                    alertBootBox(mensajes.join("<br />"), 4);
                }
                return control;
            }
            function alertBootBoxTitle(mensaje, titulo, callbackStringInput, size, customWidth) {
                var callbackString = (callbackStringInput != '' ? callbackStringInput : '') || '';
                var tam = (size != '' ? size : null) || null;
                var width = (customWidth != '' ? customWidth : null) || null;
                var dialog = bootbox.dialog({
                    size: tam,
                    message: mensaje,
                    title: titulo,
                    onEscape: function () {
                        eval(callbackString);
                    },
                    buttons: {
                        main: {
                            label: "Cerrar",
                            className: "btn-primary",
                            callback: function () {
                                eval(callbackString);
                            }
                        }
                    }
                });
                if (width != null) {
                    dialog.find("div.modal-dialog").width(width);
                }
                dialog.off("shown.bs.modal");
            }
            /**
             * Muestra mensaje de alerta de Bootbox
             * @param {String} mensaje El mensaje a mostrar (Puede contener HTLM)
             * @param {Number} messageLevel Nivel de alerta a mostrar (1:success, 2:information, 3:question, 4:warning, 5:error)
             * @param {String} callbackStringInput (Opcional) Codigo JS a ejecutar cuando se cierra la alerta
             * @param {String} size (Opcional) Tamaño de la alerta
             */
            function alertBootBox(mensaje, messageLevel, callbackStringInput, size) {
                messageLevel = (messageLevel == undefined) ? 0 : messageLevel;
                var titulo = "";;
                switch (messageLevel) {
                    case 1: titulo = "<span class='text-success'><i class='fa fa-check fa-2x' aria-hidden='true'></i> Correcto</span>"; break;
                    case 2: titulo = "<span class='text-primary'><i class='fa fa-info fa-2x' aria-hidden='true'></i> Información</span>"; break;
                    case 3: titulo = "<span class='text-info'><i class='fa fa-question fa-2x' aria-hidden='true'></i> Pregunta</span>"; break;
                    case 4: titulo = "<span class='text-warning'><i class='fa fa-exclamation fa-2x' aria-hidden='true'></i> Advertencia</span>"; break;
                    case 5: titulo = "<span class='text-danger'><i class='fa fa-times fa-2x' aria-hidden='true'></i> Error</span>"; break;
                }
                alertBootBoxTitle(mensaje, titulo, callbackStringInput, size);
            }
            function confirmBootBox(mensaje, callbackOkInput, callbackCancelInput, size) {
                var callbackOk = callbackOkInput || '';
                var callbackCancel = callbackCancelInput || '';
                var tam = size || null;
                bootbox.confirm({
                    size: tam,
                    message: mensaje,
                    callback: function (result) {
                        if (Boolean(result)) {
                            eval(callbackOk);
                        } else {
                            eval(callbackCancel);
                        }
                    }
                });
            }
            function alertBootstrap(alertType, htmlCode) {
                var alertBody = $("#" + alertType + "-html");
                var alertElement = $("#" + alertType + "-alert");
                alertBody.html(htmlCode);
                clearNotifications();
                alertElement.alert();
                //alertElement.fadeTo(5000, 500).slideUp(500, function () {
                //    alertElement.hide();
                //});
            }
            function showDivOnModal(divId, titulo) {
                var htmCode = $('#' + divId).html();
                alertBootBoxTitle(htmCode, titulo);
                return false;
            }
            function showDivOnModalLevel(divId, level) {
                var htmCode = $('#' + divId).html();
                alertBootBox(htmCode, level);
                return false;
            }
            function endsWith(str, suffix) {
                return str.indexOf(suffix, str.length - suffix.length) !== -1;
            }
            function startsWith(string, prefix) {
                return string.slice(0, prefix.length) == prefix;
            }
            function cloneDivWithInputs(divId) {
                $('#' + divId + ' input').each(function () {
                    $(this).attr('value', $(this).val());
                });
                var htmlCode = $('#' + divId).html();
                return htmlCode;
            }
            function ResolveUrl(url) {
                var baseUrl = '<%= Page.ResolveClientUrl("~/") %>';
                return url.replace("~/", baseUrl);
            }
            function loadUrlModal(url, args, titulo, callback, customHeightPx, customWidth) {
                var isChrome = /Chrome/.test(navigator.userAgent) && /Google Inc/.test(navigator.vendor);
                var _args = (args == undefined || args == '') ? '' : ('?' + args);
                var _argsModal = _args + (_args == '' ? '?' : '&') + 'insidemodal=true';
                var isLoading = true && !isChrome; //NO FUNCIONA EN CHROME
                var outterObject = 'onload="Loaded();" onreadystatechange="Loaded();" data="' + url + _argsModal + '" width="100%" height="' + customHeightPx + 'px"';
                var fallback = '<div class="alert alert-danger"><span>La página no pudo mostrarse en el diálogo<br/> De click <a href="' + url + args + '">aquí</a> para ir a a página solicitada</div>';
                var loading = '<h3><span class="label label-default"><i class="fa fa-cog fa-spin fa-lg" ></i> Cargando...</span></h3>'
                var object = '<object ' + outterObject + '>' + fallback + '</object>';
                var div = '<div id="divLoadingPdf">' + (isLoading ? loading : '') + '</div><div>' + object + '</div><script>function Loaded(){if (document.readyState == "complete") { setTimeout(function(){ $("#divLoadingPdf").hide(); }, 1000); }}<\/script>';
                alertBootBoxTitle(div, titulo, callback, 'large', customWidth);
            }
            function loadPdfModal(urlView, urlDownload) {
                var isChrome = /Chrome/.test(navigator.userAgent) && /Google Inc/.test(navigator.vendor);
                var isLoading = true && !isChrome; //NO FUNCIONA EN CHROME
                var urlDown = urlDownload || urlView;
                var outterObject = 'onload="Loaded();" onreadystatechange="Loaded();" data="' + urlView + '" width="100%" height="800px" type="application/pdf"';
                var innerObject = 'data="' + ResolveUrl('~/pdfjs/web/viewer.html') + '?file=' + canonicalize(urlDown) + '" width="100%" height="800px"';
                var fallback = '<div class="alert alert-danger"><span>El documento no pudo visualizarse en el navegador<br/><a class="btn btn-default btn-sm" aria-label="Left Align" href="' + urlDown + '"><span class="glyphicon glyphicon-save" aria-hidden="true"></span> Descargar</a></span></div>';
                var loading = '<h3><span class="label label-default"><i class="fa fa-cog fa-spin fa-lg" ></i> Cargando...</span></h3>'
                var object = '<object ' + outterObject + '>' + (endsWith(urlDown, ".pdf") ? '<object ' + innerObject + '>' + fallback + '</object>' : fallback) + '</object>';
                var div = '<div id="divLoadingPdf">' + (isLoading ? loading : '') + '</div><div>' + object + '</div><script>function Loaded(){if (document.readyState == "complete") { setTimeout(function(){ $("#divLoadingPdf").hide(); }, 1000); }}<\/script>';
                alertBootBoxTitle(div, 'Visualizar PDF', '', 'large');
            }
            function canonicalize(url) {
                var div = document.createElement('div');
                div.innerHTML = "<a></a>";
                div.firstChild.href = url;
                div.innerHTML = div.innerHTML;
                return div.firstChild.href;
            }
            function changeToUpperCase(controlName) {
                document.getElementById(controlName).value = document.getElementById(controlName).value.toUpperCase();
            }
            function isNumberKey(evt) {
                var charCode = (evt.which) ? evt.which : event.keyCode
                if (charCode > 31 && (charCode < 48 || charCode > 57))
                    return false;
                return true;
            }
            function downloadBytes(b64string, contentType, fileName, openInBrowser) {
                var byteString = atob(b64string);
                var ab = new ArrayBuffer(byteString.length);
                var ia = new Uint8Array(ab);
                for (var i = 0; i < byteString.length; i++) {
                    ia[i] = byteString.charCodeAt(i);
                }
                var blob = new Blob([ia], { type: contentType });
                if (!Boolean(openInBrowser)) {
                    saveAs(blob, fileName);
                } else {
                    document.location.href = window.URL.createObjectURL(blob);
                }
            }
            var keyStr = "ABCDEFGHIJKLMNOP" +
                "QRSTUVWXYZabcdef" +
                "ghijklmnopqrstuv" +
                "wxyz0123456789+/" +
                "=";
            function decode64(input) {
                var output = "";
                var chr1, chr2, chr3 = "";
                var enc1, enc2, enc3, enc4 = "";
                var i = 0;

                do {
                    enc1 = keyStr.indexOf(input.charAt(i++));
                    enc2 = keyStr.indexOf(input.charAt(i++));
                    enc3 = keyStr.indexOf(input.charAt(i++));
                    enc4 = keyStr.indexOf(input.charAt(i++));

                    chr1 = (enc1 << 2) | (enc2 >> 4);
                    chr2 = ((enc2 & 15) << 4) | (enc3 >> 2);
                    chr3 = ((enc3 & 3) << 6) | enc4;

                    output = output + String.fromCharCode(chr1);

                    if (enc3 != 64) {
                        output = output + String.fromCharCode(chr2);
                    }
                    if (enc4 != 64) {
                        output = output + String.fromCharCode(chr3);
                    }

                    chr1 = chr2 = chr3 = "";
                    enc1 = enc2 = enc3 = enc4 = "";

                } while (i < input.length);

                return unescape(output);
            }
            function getBase64(file, callback) {
                var reader = new FileReader();
                reader.readAsDataURL(file);
                reader.onload = function () {
                    var result = reader.result;
                    callback(result, file.name);
                };
            }
        </script>
    </form>
</body>
</html>
